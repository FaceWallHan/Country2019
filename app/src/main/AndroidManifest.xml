<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.end.summer">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".AppClient"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SplashActivity"
            android:screenOrientation="landscape">
            <intent-filter >
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".activity.LoginActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".activity.IpSettingActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".activity.SettingActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".activity.WarnWeatherActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".activity.CheckActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".activity.WebCheckActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".activity.EtcInfoActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".activity.UserCenterActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".activity.CollectionActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".activity.ViolationInfoActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".activity.PayActivity"
            android:screenOrientation="landscape"/>
    </application>

</manifest>